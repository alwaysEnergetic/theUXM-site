{
  "name": "kookjs-client",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "",
  "workspaces": [
    "packages/*",
    "modules/*"
  ],
  "scripts": {
    "build": "NODE_ENV=production yarn wpack:dev --progress",
    "build:staging": "NODE_ENV=staging yarn wpack:dev --progress",
    "rm:dist": "rm -rf ./dist",
    "server:spa": "./node_modules/.bin/http-server-spa dist index.html 2201",
    "start": "webpack serve --config ./src/config/webpack/dev.config.js",
    "start:staging": "NODE_ENV=staging webpack serve --config ./src/config/webpack/dev.config.js",
    "start:spa": "NODE_ENV=development yarn server:spa & yarn wpack:dev -w",
    "wpack": "./node_modules/.bin/webpack",
    "wpack:dev": "yarn wpack --config ./src/config/webpack/dev.config.js",
    "wpack:stat": "yarn wpack:dev --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.19",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "apollo-link": "^1.2.14",
    "apollo-link-rest": "^0.8.0-beta.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.1",
    "classnames": "^2.3.1",
    "graphql": "^15.5.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.12.4",
    "http-server-spa": "^1.3.0",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",    
    "qs": "^6.10.1",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.3.2",
    "use-query-params": "^1.2.2"
  },
  "dependencies": {
    "rc-pagination": "^3.1.6",
    "react-hook-form": "^7.6.10",
    "react-image-magnify": "^2.7.4",
    "react-select": "^4.3.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-monitor": "^1.0.14"
  }
}
